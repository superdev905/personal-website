{
  "name": "mldangelo-site",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build": "node -r babel-register ./node_modules/webpack/bin/webpack -p --config ./webpack/webpack.production.config.js --progress --profile --colors && rimraf ./dist && mv ./tmp ./dist",
    "build-dev": "rimraf ./dist && babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.config.js --progress --profile --colors",
    "clean": "rimraf ./dist && rimraf ./tmp",
    "deploy": "yarn && npm run build && npm run forever-stop || true && npm run forever-start",
    "dev": "nodemon server.js --ignore app/ --ignore tests/ --exec babel-node",
    "forever-start": "forever start --minUptime 1000ms --spinSleepTime 1000ms -a -l forever.log -o out.log -e err.log -c 'nodemon server.js --exitcrash --exec babel-node' server.js",
    "forever-stop": "forever stop server.js",
    "lint": "./node_modules/.bin/eslint app models routes server.js test webpack",
    "start": "./node_modules/.bin/babel-node server",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register ./test/test.js"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "axios": "^0.15.3",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "connect": "^3.6.0",
    "connect-sqlite3": "^0.9.9",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.27.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "forever": "^0.15.3",
    "github": "9.2.0",
    "html-minify-loader": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "last.fm.api": "^0.1.3",
    "moment": "^2.18.1",
    "mongoose": "^4.9.1",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.1",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-burger-menu": "^1.10.14",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "react-ga": "^2.1.2",
    "react-google-login": "^2.8.2",
    "react-helmet": "^5.0.1",
    "react-markdown": "^2.4.6",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "stats-webpack-plugin": "^0.5.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-polyfill": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "mocha-generators": "^2.0.0",
    "nightmare": "^2.10.0"
  }
}
