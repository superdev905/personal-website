{
  "name": "mldangelo-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build-dev": "rimraf .dist && set NODE_ENV=development && webpack --config ./webpack.config.js --progress --profile --colors",
    "clean": "rimraf ./dist",
    "dev": "NODE_ENV=development && nodemon server.js --ignore app/ --exec babel-node",
    "lint": "./node_modules/.bin/eslint app routes server.js webpack.*.js || true",
    "start": "NODE_ENV=production && ./node_modules/.bin/babel-node server",
    "cluster": "NODE_ENV=production && ./node_modules/.bin/babel-node cluster"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.8",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "blocked": "^1.2.1",
    "body-parser": "^1.15.2",
    "cluster": "^0.7.7",
    "co-views": "^2.1.0",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "github": "^2.5.1",
    "html-webpack-plugin": "^2.22.0",
    "jade": "^1.11.0",
    "lodash": "^4.15.0",
    "morgan": "^1.7.0",
    "stats-webpack-plugin": "^0.4.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "css-loader": "^0.24.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "react": "^15.3.1",
    "react-burger-menu": "^1.10.4",
    "react-dom": "^15.3.1",
    "react-ga": "^2.1.1",
    "react-router": "^2.7.0",
    "sass-loader": "^4.0.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
