{
  "name": "mldangelo-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build-dev": "rimraf .dist && set NODE_ENV=development && webpack --config ./webpack.config.js --progress --profile --colors",
    "clean": "rimraf ./dist",
    "dev": "NODE_ENV=development && nodemon server.js --ignore app/ --exec babel-node",
    "lint": "./node_modules/.bin/eslint app routes server.js webpack.*.js || true",
    "start": "NODE_ENV=production && ./node_modules/.bin/babel-node server",
    "cluster": "NODE_ENV=production && ./node_modules/.bin/babel-node cluster"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "blocked": "^1.2.1",
    "body-parser": "^1.15.2",
    "co-views": "^2.1.0",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "github": "2.5.1",
    "html-webpack-plugin": "^2.24.1",
    "lodash": "^4.16.6",
    "moment": "^2.16.0",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "axios": "^0.15.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "react": "^15.3.2",
    "react-burger-menu": "^1.10.9",
    "react-dom": "^15.3.2",
    "react-ga": "^2.1.2",
    "react-router": "^3.0.0",
    "sass-loader": "^4.0.2",
    "stats-webpack-plugin": "^0.4.2",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
