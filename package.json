{
  "name": "mldangelo-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build-dev": "rimraf .dist && set NODE_ENV=development && webpack --config ./webpack.config.js --progress --profile --colors",
    "clean": "rimraf ./dist",
    "dev": "NODE_ENV=development && nodemon server.js --ignore app/ --exec babel-node",
    "lint": "./node_modules/.bin/eslint app server.js webpack.*.js",
    "start": "NODE_ENV=production && ./node_modules/.bin/babel-node server"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^3.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "blocked": "^1.2.1",
    "body-parser": "^1.15.0",
    "co-views": "^2.1.0",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "github": "^1.1.2",
    "html-webpack-plugin": "^2.15.0",
    "jade": "^1.11.0",
    "lodash": "^4.13.1",
    "stats-webpack-plugin": "^0.3.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-ga": "^2.1.0",
    "react-router": "^2.4.1",
    "sass-loader": "^3.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
