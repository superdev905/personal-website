    "babel-preset-stage-0": "^6.5.0",
    "co-views": "^2.1.0",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.15.0",
    "jade": "^1.11.0",
    "koa": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-jade": "^2.1.0",
