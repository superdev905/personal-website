{
  "name": "mldangelo-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf public/dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build-dev": "rimraf public/dist && set NODE_ENV=development && webpack --config ./webpack.config.js --progress --profile --colors",
    "clean": "rimraf public/dist",
    "dev": "NODE_ENV=development && npm run build-dev && nodemon server.js --exec babel-node",
    "lint": "./node_modules/.bin/eslint app server.js webpack.*.js",
    "start": "NODE_ENV=production && ./node_modules/.bin/babel-node server"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "blocked": "^1.2.1",
    "co-views": "^2.1.0",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flux": "^2.1.1",
    "html-webpack-plugin": "^2.15.0",
    "jade": "^1.11.0",
    "koa": "^2.0.0",
    "koa-compress": "^1.0.9",
    "koa-compressor": "^1.0.3",
    "koa-conditional-get": "^1.0.3",
    "koa-convert": "^1.2.0",
    "koa-etag": "^2.1.1",
    "koa-favicon": "^1.2.1",
    "koa-helmet": "^1.0.0",
    "koa-jade": "^2.1.0",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-proxy": "^0.5.0",
    "koa-render": "^0.2.1",
    "koa-response-time": "^1.0.2",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "koa-static-cache": "^3.1.7",
    "koa-views": "^4.1.0",
    "koa-webpack-dev-middleware": "^1.2.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "stats-webpack-plugin": "^0.3.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "nodemon": "^1.8.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
